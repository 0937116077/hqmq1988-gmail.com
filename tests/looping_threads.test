. "${srcdir=.}/init.sh"

run_prog_skip_if_failed date +%s > /dev/null
s0="$(date +%s)"

run_prog "../$NAME" "$(($TIMEOUT_DURATION/20))" 1
args="-d -f $args"
$STRACE $args || fail_ "$STRACE $args failed with code $?"
