#!/bin/sh

# Check struct flock and struct flock64 decoding.

. "${srcdir=.}/init.sh"

run_prog > /dev/null
OUT="$LOG.out"
syscalls=
for n in fcntl fcntl64; do
	$STRACE -e$n -h > /dev/null && syscalls=$syscalls,$n
done
run_strace -e trace=$syscalls $args > "$OUT"
match_diff "$OUT" "$LOG"
rm -f "$OUT"

exit 0
